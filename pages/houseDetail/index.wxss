/* houseDetail */
.movable-container{
  height: 100vh;
  width: 100vh;
  z-index: 999;
}
.movable-image{
  height: 500px;
  width: 100vh;
  top: 36%;
  left: 26%;
  margin: 0 auto;
}
.movable-image image{
  width: 100%;
  height: 100%;
}
.container {
  background: rgba(240, 245, 251, 1);
}
/* .house__detail {
  padding-bottom: 13vh;
} */
.house__info,
.house__commission,
.house__list {
  background: rgba(255, 255, 255, 1)
}
.house__info image {
  width: 100%;
  height: 200px;
  display: block;
  object-fit: cover;
}
.house__info .house__name {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.house__info .house__name text {
  font-size: 24px;
  font-family: '苹方-简 常规体';
  margin-right: 10px
}
.house__money {
  color: rgba(231, 76, 60, 1);
  font-size: 16px;
  margin-top: 5px;
}
.info__list {
  color: rgba(0, 0, 0, 1);
  font-size: 14px;
  display: grid;
  grid-template-rows: repeat(3, 1fr);
  grid-template-columns: repeat(8fr, 4fr, 1fr);
  align-items: center;
  margin-top: 10px;
}
.info__list .list__name {
  color: rgba(0, 0, 0, 0.45)
}
.info__list image {
  width: 28px;
  height: 28px
}
.house__commission {
  margin-top: 8px;
  padding: 15px;
}
.commission__est {
  font-size: 18px;
}
.commission__tag {
  display: flex;
  justify-content: flex-start;
  /* margin: 8px 0; */
}
.commission__pla,
.commission__ord {
  margin: 10px 10px 0 0;
}
.commission__pla text,
.commission__ord text {
  padding: 2px 5px;
  font-size: 11px;
}
.pla1, .ord1 {
  background: rgba(176, 130, 78, 1);
  border: 1px solid rgba(176, 130, 78, 1);
  border-radius: 3px 0 0 3px;
  color: rgba(255, 255, 255, 1);
  font-family: '苹方-简 常规体';
}
.pla2, .ord2 {
  background: rgba(243, 156, 18, 0.45);
  border: 1px solid rgba(139, 87, 42, 0.65);
  border-radius: 0 3px 3px 0;
  color: rgba(139, 87, 42, 1);
  font-family: '苹方-简 中黑体';
}
.ord2 {
  background: rgba(255, 199, 69, 0.15);
}
.house__list {
  margin-top: 8px;
  padding: 10px 15px 13vh 15px;
}
.house__list .list__top,
.list__top view {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list__top text {
  font-size: 13px;
  color: rgba(0, 0, 0, 0.65);
}
.list__top image {
  width: 28px;
  height: 28px;
}
.van-tab--active {
  color: rgba(139, 87, 42, 1)!important
}
.preview__popup {
  width: 100%;
  height: auto
}
.preview__img {
  display: block;
  width: 100vw
}