body{font-family:PingFangSC-Regular,Microsoft Yahei, Hiragino Sans GB,Arial,Helvetica,"微软雅黑","宋体",sans-serif;background:#F5F5F5;font-size:24rpx}
body,div,ul,li,input,button,textarea,p{margin:0;padding:0}
ul{list-style:none}
textarea{overflow:auto;resize:none}
input,textarea,button,select{outline:none;appearance: none;-webkit-appearance: none;}
input,a,img{border:0;}
input,button,a{-webkit-tap-highlight-color: rgba(0,0,0,0); -webkit-tap-highlight-color: transparent;}
.displayflex{display: -webkit-box; display: -webkit-flex;display: flex;}
.flexli{-webkit-box-flex: 1; -moz-box-flex: 1;-webkit-flex: 1;flex: 1;}
.fl{float:left;}
.fr{float:right;}
.clearfix{zoom:1}
.clearfix:after{display:block;visibility:hidden;clear:both;height:0;content:'.';font-size:0}
.border_bottom{position: relative;}
.border_bottom::after{content:"";position: absolute;bottom:-1rpx;width:718rpx;left:0;height:2rpx;background: rgba(245,245,245,1);}
.split{position: relative;margin-top:14rpx;}
.split::before{content:"";position: absolute;top:-14rpx;width:750rpx;left:0;height:14rpx;background:#f5f5f5;left:-32rpx;}
.topjianbian{background:#EC4B39;background:-moz-linear-gradient(right,#EC4B39,#FF2C52);background:-webkit-gradient(linear,0 50%,100% 50%,from(#EC4B39),to(#FF2C52));
    background:-webkit-linear-gradient(right,#EC4B39,#FF2C52);background:-o-linear-gradient(right,#EC4B39,#FF2C52);}

.calWrap{width:750rpx;margin:0 auto;}
.cal_top{width: 750rpx;height:392rpx; position: relative;}
.cal_nav{width:360rpx;height:58rpx;position: relative;top:20rpx;margin:0 auto;}
.cal_nav .cal_changenav{width: 50%;font-size: 26rpx;color:#fff;text-align: center;line-height: 54rpx;border:2rpx solid #fff;}
.cal_nav .cal_changenav[data-name="benjin"]{border-radius: 0 6rpx 6rpx 0;}
.cal_nav .cal_changenav[data-name="benxi"]{border-radius: 6rpx 0 0 6rpx;border-right:0;}
.cal_nav .cal_changenav.current{background: #fff;color:#EA4C40;}
.cal_result{position: relative;top:49rpx;padding: 0 33rpx;height:196rpx;}
.cal_result .cal_resultli{width: 50%;}
.cal_result .cal_resultli .cal_title{font-size:22rpx;color:#fff;margin-bottom: 5rpx;height:29rpx;}
.cal_result .cal_resultli .cal_price{font-size:60rpx;color:#fff;width: 100%;overflow: hidden;display: block;white-space: nowrap;text-overflow: ellipsis;height:79rpx;}
.cal_result .cal_resultli .cal_hktotal{
    /* width:100%; */
    border-right:1rpx solid #fff;margin-top:28rpx;height:40rpx;line-height: 40rpx;color:#fff;overflow: hidden;display: block;white-space: nowrap;text-overflow: ellipsis;}
.cal_result .cal_resultli .cal_hktotal .htotalnum,.cal_result .cal_resultli .cal_hktotal .htotallixinum{font-size:30rpx;}
.cal_result .cal_resultli .cal_hktotal.cal_right{border-right: 0;text-indent: 20rpx;}
.cal_top .cal_benxiinfo{height:68rpx;position: absolute;bottom: 0;width:686rpx;left:0;right:0;margin:auto;border-top:1rpx solid rgba(255,255,255,1);}
.cal_benxiinfo .cal_bxfont{height:68rpx;line-height: 68rpx;display: inline-block;color:#fff;font-size: 26rpx;text-decoration: none;}
.cal_benxiinfo .cal_bxicon{display: inline-block;width:18rpx;height:18rpx;vertical-align: middle;position: relative;border-top: 2rpx solid #fff;border-right: 2rpx solid #fff;transform: rotate(45deg);-webkit-transform: rotate(45deg);margin-left:5rpx;}


.cal_form{margin-top:20rpx;background: #fff;padding:0 32rpx;}
.cal_form .cal_li{height:90rpx;}
.cal_form .cal_li .cal_leftname{line-height:86rpx;color:#333;font-size: 30rpx;float:left;}
.cal_form .cal_li .cal_rightselect{line-height:86rpx;color:#666;font-size: 26rpx;float:right;}
.cal_form .cal_li .cal_rightselect .lanchange{line-height:84rpx;color:#666;font-size: 26rpx;border:0; height:84rpx;background: transparent;text-align: right;direction: rtl;}
.cal_form .cal_li .cal_rightselect .lanchange option { direction: rtl;text-align: right;}
.lanchange,.rightrow,.cal_rightselect input,.cal_rightselect span{display: inline-block;vertical-align: middle;}
.cal_rightselect input::-webkit-input-placeholder{color:#A8A8A8;}
.cal_form .cal_li .cal_rightselect .rightrow{width:15rpx;height:15rpx;border-top: 2rpx solid rgba(82,82,82,1);border-right: 2rpx solid rgba(82,82,82,1);transform: rotate(45deg);-webkit-transform: rotate(45deg);}
.cal_form .cal_li .cal_rightselect .lannum{width:150rpx;height:80rpx;line-height: 80rpx;border:0;text-align: right;font-size: 26rpx;color:#666;}
.calWrap .tips{color:#999;font-size:22rpx;margin-top:14rpx;padding-left:31rpx;}
/* .gjj_hook{display: none;} */
.cal_dijian,.cal_dijianprice{text-indent: 40rpx;}

.infoDialog{position: absolute;top:0;right:0;left:0;bottom: 0;z-index: 500;overflow-y: scroll; background: #fff;}
.infoinner{width:750rpx;margin:0 auto;position: relative;}
.infoinner .infotop{width:750rpx;height:218rpx;position: relative;}
.infoinner .infotop .infototal{position: absolute;width:95%;height:95rpx;bottom: 15rpx;margin:0 auto;left:0;right:0;}
.infoinner .infotop .infototal .infotli{width:25%;text-align: center;color:#fff;}
.infoinner .infotop .infototal .infotli .name{font-size:22rpx;}
.infoinner .infotop .infototal .infotli .price{font-size:30rpx;width:100%;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;margin-top:10rpx;}
.tablenav{border-bottom:2rpx solid #F5F5F5;background-color: #fff;box-shadow: 0 7rpx 7rpx rgba(0,0,0,0.1);}
.tablenavinner{padding:0 10rpx;height:70rpx;}
.tablenavinner .fixtitle{line-height: 70rpx;text-align: center;font-size: 26rpx;color:#333;}

.oneyear .yeartitle{background: #F5F5F5;height:50rpx;line-height: 50rpx;font-size: 26rpx;color:#666;text-indent: 70rpx;}
.oneyear .mounths{color:#333;font-size:26rpx;padding: 0 10rpx;}
.oneyear .mounths .mouthli{height: 88rpx;}
.oneyear .mounths .mouthli::after{left:22rpx;}
.oneyear .mounths .mouthli .mouthtd{display: table;width:25%;height:88rpx;}
.oneyear .mounths .mouthli .mouthtd span{display: table-cell;vertical-align: middle;text-align: center;width: 100%;height:100%;word-break: break-all;}
.topfixedpc {position: fixed;width: 100%;left: 0;top: 0;z-index: 1000;}



@-webkit-keyframes fadeInRight {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(100%,0,0);
        transform: translate3d(100%,0,0)
    }

    100% {
        opacity: 1;
        -webkit-transform: none;
        transform: none
    }
}

@keyframes fadeInRight {
    0% {
        opacity: 0;
        -webkit-transform: translate3d(100%,0,0);
        -ms-transform: translate3d(100%,0,0);
        transform: translate3d(100%,0,0)
    }

    100% {
        opacity: 1;
        -webkit-transform: none;
        -ms-transform: none;
        transform: none
    }
}

.fadeInRight {
    -webkit-animation: fadeInRight .3s ease;
    animation: fadeInRight .3s ease
}


@-webkit-keyframes fadeInLeft {
    0% {
        opacity: 1;
        -webkit-transform: none;
        transform: none
    }

    100% {
        opacity: 0;
        -webkit-transform: translate3d(100%,0,0);
        transform: translate3d(100%,0,0)
    }
}

@keyframes fadeInLeft {
    0% {
        opacity: 1;
        -webkit-transform: none;
        -ms-transform: none;
        transform: none
    }

    100% {
        opacity: 0;
        -webkit-transform: translate3d(100%,0,0);
        -ms-transform: translate3d(100%,0,0);
        transform: translate3d(100%,0,0)
    }
}

.fadeInLeft {
    -webkit-animation: fadeInLeft .3s ease;
    animation: fadeInLeft .3s ease
}



















