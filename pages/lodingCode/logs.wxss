
.container {
  background: rgba(240, 245, 251, 1);
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.container image {
  width: 256px;
  height: 256px;
  margin-top: 48px;
}
.more__code {
  color: rgba(0, 0, 0, 0.65);
  font-size: 14px;
  background: #ffffff;
  border: solid 1px rgba(0, 0, 0, 0.15);
  border-radius: 24px;
  height: 36px;
  width: 209px;
  text-align: center;
  line-height: 36px;
}
.lod__code {
  color: rgba(139, 87, 42, 1);
  font-size: 18px;
  margin-top: 16px;
}

