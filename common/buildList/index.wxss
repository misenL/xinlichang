.commission__tag {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 14px;
  margin-top: 10px;
  /* margin: 8px 0; */
}
.commission__label {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.commission__pla,
.commission__ord {
  margin: 10px 10px 0 0;
  display: flex;
}
.commission__pla text,
.commission__ord text {
  padding: 2px 5px;
  font-size: 11px;
  line-height: 6px;
  height: 18px;
}
.pla1, .ord1 {
  border-radius: 3px 0 0 3px;
  color: rgba(255, 255, 255, 1);
  font-family: '苹方-简 常规体';
}
.pla2, .ord2 {
  border-radius: 0 3px 3px 0;
  font-family: '苹方-简 中黑体';
}



