.van-image {
  display: block!important;
}
.build__state{
  font-size: 11px;
  color: rgba(0, 0, 0, .65);
  display: block;
  width: auto;
}
.build__list {
  background: rgb(255, 255, 255);
  width: 100vw;
  padding: 12px;
  border-bottom: 1px solid #F0F5FB
}
.build__info {
  display: flex;
  justify-content: flex-start;
}
.info__text {
  padding-left: 12px;
}
.build__name {
  font-size: 16px;
  font-family: '苹方-简 常规体';
  margin-right: 10px
}
.build__money {
  font-size: 16px;
  color: #E74C3C;
  display: block;
  margin-top: 2px;
}
.build__loc {
  font-size: 11px;
  color: rgba(0, 0, 0, .65);
  display: block;
  width: auto;
  margin-top: 8px;
}
.build__tag {
 display: flex;
 justify-content: flex-start;
 flex-wrap: wrap;
}
.build__tag view {
  margin: 5px 10px 2px 0;
}
.build__tag text {
  padding: 2px 5px;
  font-size: 11px;
}

/* 标签颜色匹配 */
/* 住宅 */
.tag_view_0 text {
  border: 1px solid rgba(39, 174, 96, 1);
}
.text_name_0 {
  border-radius: 3px 0 0 3px;
  background: rgba(39, 174, 96, 1);
  color: rgba(255, 255, 255, 1)
}
.text_num_0 {
  border-radius: 0 3px 3px 0;
  background: rgba(39, 174, 96, 0.25);
  color: rgba(39, 174, 96, 1);
}
